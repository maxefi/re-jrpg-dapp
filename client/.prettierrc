{
  "proseWrap": "never",
  "parser": "typescript",
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "semi": true,
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 120,
  "endOfLine": "lf"
}
